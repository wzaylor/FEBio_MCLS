	keywords = {Knee, patient-specific modeling, Laxity, limits of passive motion},
	file = {Full Text PDF:/home/will/Zotero/storage/J8VSP2AK/Roth et al. - 2015 - The limits of passive motion are variable between .pdf:application/pdf;Snapshot:/home/will/Zotero/storage/E7HKE3ZN/abstract.html:text/html}
