	keywords = {Knee, Kneeling, Ligament forces, Model, Squatting},
	file = {Springer Full Text PDF:/home/will/Zotero/storage/KJ5K5ELI/Yang et al. - 2010 - Development of a subject-specific model to predict.pdf:application/pdf}
}

