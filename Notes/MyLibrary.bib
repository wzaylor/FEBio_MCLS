@article{amiri_computational_2012,
	title = {A Computational Modeling Approach for Investigating Soft Tissue Balancing in Bicruciate Retaining Knee Arthroplasty.},
	volume = {2012},
	issn = {1748-670X, 1748-670X},
	url = {http://www.hindawi.com/journals/cmmm/2012/652865/abs/, http://www.hindawi.com/journals/cmmm/2012/652865/abs/},
	doi = {10.1155/2012/652865, 10.1155/2012/652865},
	abstract = {Bicruciate retaining knee arthroplasty, although has shown improved functions and patient satisfaction compared to other designs of total knee replacement, remains a technically demanding option for treating severe cases of arthritic knees. One of the main challenges in bicruciate retaining arthroplasty is proper balancing of the soft tissue during the surgery. In this study biomechanics of soft tissue balancing was investigated using a validated computational model of the knee joint with high fidelity definitions of the soft tissue structures along with a Taguchi method for design of experiments. The model was used to simulate intraoperative balancing of soft tissue structures following the combinations suggested by an orthogonal array design. The results were used to quantify the corresponding effects on the laxity of the joint under anterior-posterior, internal-external, and varus-valgus loads. These effects were ranked for each ligament bundle to identify the components of laxity which were most sensitive to the corresponding surgical modifications. The resulting map of sensitivity for all the ligament bundles determined the components of laxity most suitable for examination during intraoperative balancing of the soft tissue. Ultimately, a sequence for intraoperative soft tissue balancing was suggested for a bicruciate retaining knee arthroplasty., Bicruciate retaining knee arthroplasty, although has shown improved functions and patient satisfaction compared to other designs of total knee replacement, remains a technically demanding option for treating severe cases of arthritic knees. One of the main challenges in bicruciate retaining arthroplasty is proper balancing of the soft tissue during the surgery. In this study biomechanics of soft tissue balancing was investigated using a validated computational model of the knee joint with high fidelity definitions of the soft tissue structures along with a Taguchi method for design of experiments. The model was used to simulate intraoperative balancing of soft tissue structures following the combinations suggested by an orthogonal array design. The results were used to quantify the corresponding effects on the laxity of the joint under anterior-posterior, internal-external, and varus-valgus loads. These effects were ranked for each ligament bundle to identify the components of laxity which were most sensitive to the corresponding surgical modifications. The resulting map of sensitivity for all the ligament bundles determined the components of laxity most suitable for examination during intraoperative balancing of the soft tissue. Ultimately, a sequence for intraoperative soft tissue balancing was suggested for a bicruciate retaining knee arthroplasty.},
	pages = {652865},
	journaltitle = {Computational and Mathematical Methods in Medicine},
	author = {Amiri, Shahram and Wilson, David R. and Amiri, Shahram and Wilson, David R.},
	urldate = {2016-04-08},
	date = {2012-10-02},
	langid = {english},
	file = {Full Text PDF:/home/will/Zotero/storage/NUQI6FDC/Amiri et al. - 2012 - A Computational Modeling Approach for Investigatin.pdf:application/pdf;Snapshot:/home/will/Zotero/storage/N4XM2KBI/652865.html:application/xhtml+xml}
}

@article{gee_computational_2010,
	title = {A computational strategy for prestressing patient-specific biomechanical problems under finite deformation},
	volume = {26},
	rights = {Copyright © 2009 John Wiley \& Sons, Ltd.},
	issn = {2040-7947},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1002/cnm.1236},
	doi = {10.1002/cnm.1236},
	abstract = {In simulation of biomechanical structures the patient-specific geometry of the object of interest is very often reconstructed from in vivo medical imaging such as {CT} scans. Such geometries therefore represent a deformed configuration stressed by typical in vivo conditions. Commonly, such structures are considered stress free in simulation. In this contribution we present and compare two methods to introduce a physically meaningful stress/strain state to the obtained geometry for simulations in the finite strain regime and demonstrate the necessity of such prestressing techniques. One method is based on an inverse design analysis to calculate a stress-free reference configuration. The other method developed here is based on a modified updated Lagrangian formulation. The formulation of both methods is provided in detail and implementation issues are discussed. Applicability and accurateness of both approaches are compared and evaluated utilizing an analytical aorta model and fully three-dimensional patient-specific abdominal aortic aneurysm structures in the finite strain regime. Copyright © 2009 John Wiley \& Sons, Ltd.},
	pages = {52--72},
	number = {1},
	journaltitle = {International Journal for Numerical Methods in Biomedical Engineering},
	author = {Gee, M. W. and Forster, Ch and Wall, W. A.},
	urldate = {2018-10-25},
	date = {2010-01-01},
	langid = {english},
	keywords = {abdominal aortic aneurysm, biomechanics, inverse design analysis, patient-specific modeling, prestraining, prestressing},
	file = {Full Text PDF:/home/will/Zotero/storage/M6BF6QK7/Gee et al. - 2010 - A computational strategy for prestressing patient-.pdf:application/pdf;Snapshot:/home/will/Zotero/storage/E37J8Z74/cnm.html:text/html}
}

@article{weiss_three-dimensional_2005,
	title = {Three-dimensional finite element modeling of ligaments: Technical aspects},
	volume = {27},
