	keywords = {total knee arthroplasty, joint distraction force, soft tissue balance},
	file = {ScienceDirect Full Text PDF:/home/will/Zotero/storage/JR3VB686/Nagai et al. - 2014 - Soft Tissue Balance Changes Depending on Joint Dis.pdf:application/pdf;ScienceDirect Snapshot:/home/will/Zotero/storage/285NGNUV/S0883540313005433.html:text/html}
