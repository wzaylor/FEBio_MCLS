	keywords = {Gait, cartilage, mechanics, knee kinematics, 3-dimensional model, anterior-posterior, automatic-determination, Computational biomechanics, Elastic foundation   model, gait, Knee mechanics, popliteofibular ligament, quadriceps, stability},
	file = {Full Text PDF:/home/will/Zotero/storage/THS545CT/Lenhart et al. - 2015 - Prediction and Validation of Load-Dependent Behavi.pdf:application/pdf;Snapshot:/home/will/Zotero/storage/2M3GEHUW/10.html:text/html}
