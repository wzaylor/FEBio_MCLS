	langid = {english},
	keywords = {anatomy, posterior cruciate ligament, Fibre bundles, length changes, meniscofemoral ligament},
	file = {Full Text PDF:/home/will/Zotero/storage/MAQ9XUAH/Amis et al. - 2005 - Anatomy of the posterior cruciate ligament and the.pdf:application/pdf;Snapshot:/home/will/Zotero/storage/AJFTMQSZ/10.html:text/html}
