	date = {2016-02},
	pmid = {27077585},
	keywords = {Humans, Anterior Cruciate Ligament, Anterior Cruciate Ligament Reconstruction, Cone-Beam Computed Tomography, Knee Injuries, Knee Joint, Magnetic Resonance Imaging, Postoperative Period, Treatment Outcome},
	file = {Van Dyck et al. - 2016 - Imaging of the Postoperative Anterior Cruciate Lig.pdf:/home/will/Zotero/storage/E275243P/Van Dyck et al. - 2016 - Imaging of the Postoperative Anterior Cruciate Lig.pdf:application/pdf}
